Include config-local

Match host github-ethulhu !exec "util is-ssh-connection"
	IdentityAgent ~/.gnupg/S.gpg-agent.ssh

Host github-ethulhu
	HostName github.com
	User git


Match host "*.eth.moe,*.ethulhu.co.uk" !exec "util is-ssh-connection"
	IdentityAgent ~/.gnupg/S.gpg-agent.ssh

Host *.eth.moe *.ethulhu.co.uk
	ForwardAgent yes
